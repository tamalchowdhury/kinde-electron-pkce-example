# Copy this file to .env and fill in your values.
# Kinde issuer looks like: https://your_tenant.kinde.com
KINDE_ISSUER_URL=https://your-tenant.kinde.com
KINDE_CLIENT_ID=your-native-client-id
# Optional audience if you have APIs configured in Kinde (leave blank if not)
KINDE_AUDIENCE=
# Comma separated scopes; 'offline' includes a refresh token
KINDE_SCOPES=openid profile email offline
